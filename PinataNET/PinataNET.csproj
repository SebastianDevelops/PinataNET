<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>PinataNET</Title>
    <Authors>Sebastian Van Rooyen</Authors>
    <Company />
    <Description>Simple c# wrapper for the Pinata api</Description>
    <PackageProjectUrl>https://github.com/SebastianDevelops/PinataNET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/SebastianDevelops/PinataNET</RepositoryUrl>
    <PackageTags>pinata,ipfs</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MimeTypesMap" Version="1.0.8" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>

</Project>
